{
	"name": "synthwave-stack",
	"packageManager": "yarn@3.2.1",
	"private": true,
	"description": "",
	"license": "MIT",
	"sideEffects": false,
	"scripts": {
		"build": "run-s 'build:*'",
		"build:css": "npm run generate:css -- --minify",
		"build:remix": "cross-env NODE_ENV=production remix build",
		"dev": "run-p 'dev:*'",
		"dev:css": "npm run generate:css -- --watch",
		"dev:remix": "cross-env NODE_ENV=development remix dev",
		"format": "prettier --write .",
		"generate:css": "tailwindcss -i ./styles/tailwind.css -o ./app/tailwind.css",
		"lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
		"prepare": "husky install",
		"postinstall": "remix setup node",
		"typecheck": "tsc -b"
	},
	"dependencies": {
		"@aws-sdk/client-s3": "3.199.0",
		"@aws-sdk/lib-storage": "3.199.0",
		"@headlessui/react": "1.7.3",
		"@prisma/client": "4.5.0",
		"@radix-ui/react-accordion": "1.0.1",
		"@radix-ui/react-alert-dialog": "1.0.2",
		"@radix-ui/react-avatar": "1.0.1",
		"@radix-ui/react-checkbox": "1.0.1",
		"@radix-ui/react-dialog": "1.0.2",
		"@radix-ui/react-dropdown-menu": "2.0.1",
		"@radix-ui/react-icons": "1.1.1",
		"@radix-ui/react-navigation-menu": "1.1.1",
		"@radix-ui/react-popover": "1.0.2",
		"@radix-ui/react-radio-group": "1.1.0",
		"@radix-ui/react-switch": "1.0.1",
		"@radix-ui/react-tabs": "1.0.1",
		"@radix-ui/react-toast": "1.1.1",
		"@radix-ui/react-toggle": "1.0.1",
		"@radix-ui/react-toggle-group": "1.0.1",
		"@radix-ui/react-toolbar": "1.0.1",
		"@radix-ui/react-tooltip": "1.0.2",
		"@remix-run/node": "1.7.4",
		"@remix-run/react": "1.7.4",
		"@remix-run/vercel": "1.7.4",
		"@sendgrid/client": "7.7.0",
		"@sendgrid/mail": "7.7.0",
		"@tailwindcss/aspect-ratio": "0.4.2",
		"@vercel/node": "2.5.26",
		"aws-sdk": "2.1243.0",
		"classnames": "2.3.2",
		"formidable": "2.0.1",
		"framer-motion": "7.6.2",
		"nanoid": "4.0.0",
		"nprogress": "0.2.0",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"react-feather": "2.0.10",
		"react-icons": "4.6.0",
		"remix": "^1.7.4",
		"remix-auth": "3.3.0",
		"remix-auth-email-link": "1.5.0",
		"remix-auth-google": "1.1.0",
		"remix-auth-twitter": "0.1.0",
		"tailwindcss-radix": "2.6.1",
		"zod": "^3.19.1",
		"zodix": "^0.2.0"
	},
	"devDependencies": {
		"@remix-run/dev": "1.7.4",
		"@remix-run/eslint-config": "1.7.4",
		"@remix-run/serve": "1.7.4",
		"@types/eslint": "8.4.9",
		"@types/formidable": "2.0.5",
		"@types/react": "18.0.24",
		"@types/react-dom": "18.0.8",
		"autoprefixer": "10.4.13",
		"concurrently": "7.5.0",
		"cross-env": "7.0.3",
		"eslint": "8.26.0",
		"eslint-config-prettier": "8.5.0",
		"husky": "8.0.1",
		"lint-staged": "13.0.3",
		"npm-run-all": "4.1.5",
		"postcss": "8.4.18",
		"prettier": "2.7.1",
		"prettier-plugin-tailwindcss": "0.1.13",
		"prisma": "4.5.0",
		"tailwindcss": "3.2.1",
		"ts-node": "10.9.1",
		"typescript": "4.8.4"
	},
	"engines": {
		"node": ">=14"
	},
	"lint-staged": {
		"*.{js,jsx,ts,tsx}": [
			"eslint --cache --fix",
			"prettier --write"
		]
	}
}
