{
	"name": "synthwave-stack",
	"packageManager": "yarn@3.2.1",
	"private": true,
	"description": "",
	"license": "MIT",
	"sideEffects": false,
	"scripts": {
		"build": "run-s 'build:*'",
		"build:css": "npm run generate:css -- --minify",
		"build:remix": "cross-env NODE_ENV=production remix build",
		"dev": "run-p 'dev:*'",
		"dev:css": "npm run generate:css -- --watch",
		"dev:remix": "cross-env NODE_ENV=development remix dev",
		"format": "prettier --write .",
		"generate:css": "tailwindcss -i ./styles/tailwind.css -o ./app/tailwind.css",
		"lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
		"prepare": "husky install",
		"postinstall": "remix setup node",
		"typecheck": "tsc -b"
	},
	"dependencies": {
		"@aws-sdk/client-s3": "^3.100.0",
		"@aws-sdk/lib-storage": "^3.100.0",
		"@headlessui/react": "^1.6.4",
		"@prisma/client": "3.13.0",
		"@radix-ui/react-accordion": "0.1.6",
		"@radix-ui/react-alert-dialog": "0.1.7",
		"@radix-ui/react-avatar": "0.1.4",
		"@radix-ui/react-checkbox": "0.1.5",
		"@radix-ui/react-dialog": "0.1.7",
		"@radix-ui/react-dropdown-menu": "0.1.6",
		"@radix-ui/react-icons": "1.1.1",
		"@radix-ui/react-navigation-menu": "0.1.2",
		"@radix-ui/react-popover": "0.1.6",
		"@radix-ui/react-radio-group": "^0.1.5",
		"@radix-ui/react-switch": "0.1.5",
		"@radix-ui/react-tabs": "0.1.5",
		"@radix-ui/react-toast": "0.1.1",
		"@radix-ui/react-toggle": "0.1.4",
		"@radix-ui/react-toggle-group": "0.1.5",
		"@radix-ui/react-toolbar": "0.1.5",
		"@radix-ui/react-tooltip": "0.1.7",
		"@remix-run/node": "1.4.3",
		"@remix-run/react": "1.4.3",
		"@remix-run/vercel": "1.4.3",
		"@sendgrid/client": "7.6.2",
		"@sendgrid/mail": "7.6.2",
		"@tailwindcss/aspect-ratio": "^0.4.0",
		"@vercel/node": "1.14.0",
		"aws-sdk": "2.1145.0",
		"formidable": "^2.0.1",
		"framer-motion": "6.3.3",
		"nanoid": "3.3.4",
		"nprogress": "^0.2.0",
		"react": "17.0.2",
		"react-dom": "17.0.2",
		"react-feather": "^2.0.10",
		"react-icons": "4.3.1",
		"remix-auth": "3.2.1",
		"remix-auth-email-link": "1.3.1",
		"remix-auth-google": "1.1.0",
		"remix-auth-twitter": "0.0.4",
		"tailwindcss-radix": "2.2.0"
	},
	"devDependencies": {
		"@remix-run/dev": "1.4.3",
		"@remix-run/eslint-config": "1.4.3",
		"@remix-run/serve": "1.4.3",
		"@types/eslint": "8.4.2",
		"@types/formidable": "^2.0.5",
		"@types/react": "17.0.24",
		"@types/react-dom": "17.0.9",
		"autoprefixer": "10.4.7",
		"concurrently": "7.1.0",
		"cross-env": "7.0.3",
		"eslint": "8.15.0",
		"eslint-config-prettier": "8.5.0",
		"husky": "8.0.1",
		"lint-staged": "12.4.1",
		"npm-run-all": "4.1.5",
		"postcss": "8.4.13",
		"prettier": "2.6.2",
		"prettier-plugin-tailwindcss": "0.1.10",
		"prisma": "3.13.0",
		"tailwindcss": "3.0.24",
		"ts-node": "10.7.0",
		"typescript": "4.5.5"
	},
	"engines": {
		"node": ">=14"
	},
	"lint-staged": {
		"*.{js,jsx,ts,tsx}": [
			"eslint --cache --fix",
			"prettier --write"
		]
	}
}
